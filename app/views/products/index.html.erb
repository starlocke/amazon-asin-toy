<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Asin</th>
      <th>Category</th>
      <th>Rank</th>
      <th>Item width</th>
      <th>Item height</th>
      <th>Item length</th>
      <th>Item width units</th>
      <th>Item height units</th>
      <th>Item length units</th>
      <th>Package width</th>
      <th>Package height</th>
      <th>Package length</th>
      <th>Package width units</th>
      <th>Package height units</th>
      <th>Package length units</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.asin %></td>
        <td><%= product.category %></td>
        <td><%= product.rank %></td>
        <td><%= product.item_width %></td>
        <td><%= product.item_height %></td>
        <td><%= product.item_length %></td>
        <td><%= product.item_width_units %></td>
        <td><%= product.item_height_units %></td>
        <td><%= product.item_length_units %></td>
        <td><%= product.package_width %></td>
        <td><%= product.package_height %></td>
        <td><%= product.package_length %></td>
        <td><%= product.package_width_units %></td>
        <td><%= product.package_height_units %></td>
        <td><%= product.package_length_units %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
